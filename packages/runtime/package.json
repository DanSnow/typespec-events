{
  "name": "@typespec-events/runtime",
  "version": "0.0.2",
  "description": "Runtime library for TypeSpec Events",
  "main": "dist/index.mjs",
  "type": "module",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "moon run build --",
    "test": "moon run test --",
    "prepare": "test -z \"$MOON_VERSION\" && moon run --no-actions build || true"
  },
  "devDependencies": {
    "@ark/attest": "0.49.0",
    "type-fest": "4.41.0",
    "typescript": "5.9.2",
    "unbuild": "3.6.1",
    "vitest": "3.2.4"
  },
  "keywords": [
    "typespec",
    "events",
    "event-tracking",
    "user-tracking",
    "tracking",
    "zod"
  ],
  "license": "MIT",
  "author": {
    "name": "DanSnow",
    "url": "https://github.com/DanSnow"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DanSnow/typespec-events"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.0.0",
    "zod": "^4.0.5"
  }
}

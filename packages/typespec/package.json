{
  "name": "@typespec-events/typespec",
  "version": "0.0.2",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "DanSnow",
    "url": "https://github.com/DanSnow"
  },
  "main": "dist/index.js",
  "tspMain": "lib/main.tsp",
  "files": [
    "./dist/**/*",
    "./lib/**/*",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "typespec",
    "events",
    "event-tracking",
    "user-tracking",
    "tracking",
    "zod"
  ],
  "exports": {
    ".": {
      "typespec": "./lib/main.tsp",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./decorators": {
      "types": "./dist/decorators.d.ts",
      "default": "./dist/decorators.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "default": "./dist/testing/index.js"
    }
  },
  "peerDependencies": {
    "@typespec/compiler": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "24.0.14",
    "@typespec/compiler": "latest",
    "@typespec/library-linter": "latest",
    "typescript": "^5.3.3",
    "unbuild": "^3.5.0",
    "vitest": "^3.2.4",
    "zx": "^8.7.1"
  },
  "scripts": {
    "build": "moon run build --",
    "lint": "moon run lint --",
    "format": "moon run format --",
    "test": "moon run test --",
    "prepare": "test -z \"$MOON_VERSION\" && moon run --no-actions build || true"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/DanSnow/typespec-events"
  },
  "dependencies": {
    "markdown-table": "^3.0.4",
    "scule": "^1.3.0",
    "zod": "^4.0.5"
  }
}

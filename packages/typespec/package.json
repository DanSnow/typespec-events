{
  "name": "@typespec-events/typespec",
  "version": "0.0.2",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "DanSnow",
    "url": "https://github.com/DanSnow"
  },
  "main": "dist/index.js",
  "tspMain": "lib/main.tsp",
  "files": [
    "./dist/**/*",
    "./lib/**/*",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "typespec",
    "events",
    "event-tracking",
    "user-tracking",
    "tracking",
    "zod"
  ],
  "exports": {
    ".": {
      "typespec": "./lib/main.tsp",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./decorators": {
      "types": "./dist/decorators.d.ts",
      "default": "./dist/decorators.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "default": "./dist/testing/index.js"
    }
  },
  "peerDependencies": {
    "@typespec/compiler": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "24.3.0",
    "@typespec/compiler": "latest",
    "@typespec/library-linter": "latest",
    "proper-tags": "2.0.2",
    "typescript": "5.8.3",
    "unbuild": "3.6.1",
    "vitest": "3.2.4",
    "zx": "8.7.1"
  },
  "scripts": {
    "build": "moon run build --",
    "lint": "moon run lint --",
    "format": "moon run format --",
    "test": "moon run test --",
    "prepare": "test -z \"$MOON_VERSION\" && moon run --no-actions build || true"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
  "repository": {
    "type": "git",
    "url": "https://github.com/DanSnow/typespec-events"
  },
  "dependencies": {
    "markdown-table": "^3.0.4",
    "scule": "^1.3.0",
    "tiny-invariant": "^1.3.3",
    "zod": "^4.0.5"
  }
}
